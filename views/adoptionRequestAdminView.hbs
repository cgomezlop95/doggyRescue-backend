<h3>Approved Adoption Requests</h3>
<ul>
  {{#each adoptionRequests}}
    {{#if this.requestApproved}}
      <li>
        <a href="/adoption-requests/{{this.userId}}_{{this.dogId}}">
          {{this.user.firstName}}
          {{this.user.lastName}}
          has adopted
          {{this.dog.dogName}}
        </a>
      </li>
    {{/if}}
  {{/each}}
</ul>